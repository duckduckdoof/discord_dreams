{
    "extract_audio": true,
    "format": "bestaudio/best",
    "outtmpl": "%(extractor)s-%(id)s-%(title)s.%(ext)s",
    "restrictfilenames": true,
    "noplaylist": true,
    "nocheckcertificate": true,
    "ignoreerrors": false,
    "logtostderr": false,
    "quiet": true,
    "no_warnings": true,
    "default_search": "auto",
    "force_ipv4": true,
    "source_address": "0.0.0.0"
}
